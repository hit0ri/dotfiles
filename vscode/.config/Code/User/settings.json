{
  "editor.cursorBlinking": "solid",
  "editor.fontFamily": "'Fira Mono', 'Source Code Pro', 'monospace', monospace, 'Droid Sans Fallback', 'Noto Color Emoji'",
  "editor.fontSize": 14,
  "editor.minimap.enabled": false,
  "editor.occurrencesHighlight": false,
  "editor.renderLineHighlight": "gutter",
  "files.associations": {
    "inventory": "ini",
    "Vagrantfile": "ruby"
  },
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/_site": true,
    "**/tmp": true,
    "**/*.pyc": true,
    "**/*.asset-cache": true,
    "**/*.cache": true,
    "**/*.jekyll-cache": true,
    "**/*.tweet-cache": true,
    "**/__pycache__": true,
    "**/.pytest_cache": true,
    "**/*.egg-info": true,
    "**/*.coverage": true,
    "**/celerybeat-schedule": true,
    "**/node_modules": true,
    "**/bower_components": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.fontFamily": "'Fira Mono', 'Source Code Pro', monospace",
  "window.zoomLevel": 1,
  "workbench.colorTheme": "One Dark Pro",
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "none"
}
