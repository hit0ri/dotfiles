"*":
  core:
    autoHideMenuBar: true
    automaticallyUpdate: false
    telemetryConsent: "no"
    themes: [
      "one-dark-ui"
      "gruvbox-plus-syntax"
    ]
  editor:
    fontFamily: "Fira Code, Menlo, Consolas, DejaVu Sans Mono, monospace"
  "git-plus":
    commits:
      verboseCommits: true
    general:
      splitPane: "Right"
  welcome:
    showOnStartup: false
